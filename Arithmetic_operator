num1=2
num2=3
print("The Sum of num1 and num2 is ",num1+num2)                    #Sum
print("The Subtraction of num1 and num2 is",num1-num2)             #Subtract
print("The multiplication of num1 and num2 will be ",num1*num2)    #Multiplication
print("The Division Of num1 divided by num2=",num1/num2)           #Divide
print("Num1 raised to the power of Num 2 is=",num1**num2)          #Raised to the power
print("num1 Divided by num2 is ",num1/num2)                        #Division
print("num1%num2=",num1%num2)                                      #Reaminder
