x=10
y=20
print("x<=10 and y<25",x<=10 and y<25)              #use of AND.Returns True if both statements are true.
print("x<10 or y>25",x<10 or y<25)                  #use of OR.Returns True if one of the statements is true.
print("x<10 or y>20",x<10 or y<20)
print("x<10 not y>25",not(x<=10 and y<25))          #Use of NOT.It reverse the result,returnsFalse if the result is true
